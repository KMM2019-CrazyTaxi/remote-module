<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>

<?import javafx.scene.control.Button?>
<VBox fx:id="mapRouteModule" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.controllers.MapRouteModuleController">
    <HBox>
        <Region HBox.hgrow="ALWAYS"/>
        <fx:include fx:id="mapViewFeature" source="../features/MapViewFeature.fxml" />
        <Region HBox.hgrow="ALWAYS"/>
    </HBox>

    <HBox>
        <Region HBox.hgrow="ALWAYS"/>
        <Button fx:id="newRouteButton" onMouseClicked="#handleNewRouteClick" />
    </HBox>
</VBox>